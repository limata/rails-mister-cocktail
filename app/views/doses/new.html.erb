<div class="container">
  <div class="row">
    <h1 class="text-center">Add a Dose>

    <div class="col-xs-12 col-md-offset-3 col-md-6">
      <%= simple_form_for [@cocktail, @dose] do |f| %>
        <%= f.input :description %>
        <%= f.submit class: "btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div>

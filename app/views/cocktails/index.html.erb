<div class="container">
  <div class="row">
  <%= link_to 'Create a new cocktail', new_cocktail_path(@cocktail) %>
  <h2>List of Cocktails</h2>
    <% @cocktails.each do |cocktail| %>
      <div class="col-sm-4">
        <%= link_to cocktail.name, cocktail_path(cocktail) %>
      </div>
    <% end %>
  </div>
</div>
